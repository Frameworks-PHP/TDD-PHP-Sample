<?xml version="1.0" encoding="UTF-8"?>
<phpunit
	verbose="true"
	colors="true"
	stopOnError="false"
	stopOnFailure="false"
	stopOnIncomplete="false"
	stopOnSkipped="false"
>
	<testsuites>
		<testsuite name="Main">
			<directory>./</directory>
			<exclude>./coverage-report/</exclude>
		</testsuite>
	</testsuites>
	<logging>
		<log type="coverage-html" target="./coverage-report" />
	</logging>
</phpunit>